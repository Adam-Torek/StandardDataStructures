
namespace ds::linear
{
    template<typename T>
    class dynamic_array<T> {
        private:
            unsigned int size;
            unsigned int capacity;
            T* begin;
            T* end;
            T* end_of_storage;
        public:
            using value_type = T;
            using pointer_type = T*;
            using reference_type = T&;
    };
}
